class Solution {
    public int missingNumber(int[] nums) {
        
        int n = nums.length;
        Arrays.sort(nums);
        int res;
        for(int i = 0; i<n ; i++){
            if(nums[i] != 0){
                res = i;
            }   
                break;
        }
        return res;
    }
}

<______________________________________error: variable res might not have been initialized
        return res;
               ^__________________________________________>
